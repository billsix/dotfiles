[core]
	editor = emacs -nw
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
        squash = "!f() { git reset --soft $1 && git commit ${2:+-m \"$2\"}; }; f"
[log]
	decorate = true
[pull]
	rebase = true
[rebase]
	autoStash = true
